{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
					 "${workspaceFolder}",
					 "${env:REPO_DIR}/tws-api/source/cppclient/client",
					 "${env:REPO_DIR}/spdlog/include",
					 "${env:REPO_DIR}/json/include"
            ],
            "defines": [],
            "intelliSenseMode": "clang-x64",
            "browse": { "path": ["${workspaceFolder}", "${env:REPO_DIR}/llvm-project/libcxx/include" ], "limitSymbolsToIncludedHeaders": true, "databaseFilename": "" },
				"forcedInclude": ["pc.h"],
            "compilerPath": "/usr/local/bin/clang-12",
            "cppStandard": "c++20"
        },
        {
            "name": "Win32",
            "includePath": [
                "C:/Program Files (x86)/Microsoft Visual Studio 14.0/VC/include",
                "${workspaceFolder}"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "intelliSenseMode": "msvc-x64",
            "browse": {
                "path": [
                    "C:/Program Files (x86)/Microsoft Visual Studio 14.0/VC/include/*",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}